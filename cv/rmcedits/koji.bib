@inproceedings{tominaga_modelling_2008,
	Abstract = {Acid deposition may cause persistent adverse effects in terrestrial and freshwater ecosystems. The extent of acidification depends on the historical, current and future acid deposition levels as well as the geological sensitivity in the region. Even though acid deposition has decreased in most areas in North America, recovery (biological and chemical) has been limited due to complex biogeochemical processes. 
We employed {MAGIC} (Model of Acidification in Groundwater in Catchments), a dynamic acidification model to a forested sub-catchment at Hubbard Brook, New Hampshire, {USA}, to predict the changes in soil and stream chemistry in response to proposed emission scenarios (further reductions of 38 \% ({SO}2) and 47 \% ({NOx}) of the 1990 levels by 2020). Model predictions depend strongly on model calibration, and calibration depends on available observational data. Differences between modelled and observed data in general decrease with increasing length of the time series used in calibration. {MAGIC} was calibrated to observed stream chemistry (1971--2004) using {PEST} (Parameter Estimate), which is an objective model-independent calibration technique (weighted, lumped observation-to-model correlation r = 0.97). At this site, stream chemistry has acidified and is slowly recovering, while soil chemistry has shown no improvement. Stream acid neutralising capacity is predicted to rise above zero by 2013; nonetheless, soil base saturation recovery is very slow (less than 10 \%, a 1971 level, in 2100). Our findings suggest that the slow recovery of soil chemistry reflects limited natural recovery process (mineral weathering), rather than unsatisfactory reduction in acid deposition.},
	Address = {Gananoque, Ontario, Canada},
	Author = {Tominaga, Koji and Aherne, Julian and Watmough, Shaun A.},
	Month = feb,
	Note = {22nd Annual Gananoque Conference on Environmental Science and Engineering. Oral presentation. Presenter.},
	Title = {Modelling the chemistry of soil and surface water at Hubbard Brook, New Hampshire, {USA}},
	Year = {2008}}

