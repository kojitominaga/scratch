@inproceedings{tominaga_future_2012,
	Abstract = {In recent decades, many lakes in temperate regions have experienced increased water temperature and shortened duration of ice cover. These physical changes are in turn deemed to impact lake biota, for example by interfering with food chain dynamics or by dictating species shift. Here we produce a forecast of lake physical response to changing climate by connecting a dynamically downscaled weather scenario for Fennoscandia to a physically-based lake model ({MyLake}). In order to facilitate systematic representation of the lakes in the region, we designed the simulation using 27 generic lake types with varying depth, area and colour levels. Lake responses such as ice duration and water temperature were then simulated both for the control period 1970-2000 and the scenario period 2070-2100. The model predicts striking pan-regional changes over the 100 years: shorter ice duration (-50 days), higher epilimnion water temperature (+1.65 K annual mean), and {\textgreater} 10 \% increase in irradiance reaching the water surface. The magnitude of change varies geographically, with the most vulnerable areas being in southern or more oceanic areas where ice thinning accelerates water heating.},
	Address = {Otsu, Shiga, Japan},
	Author = {Tominaga, Koji and Finstad, Anders G. and Kaste, {\O}yvind and Andersen, Tom},
	Month = jul,
	Note = {2012 {ASLO} Aquatic Sciences Meeting. Oral presentation. Presenter.},
	Title = {Future climate change impact on lakes in Fennoscandia: A dynamic mechanistic approach},
	Year = {2012}}

